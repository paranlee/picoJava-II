set_drive DRIVE_OF_INV1 all_inputs()
set_load LOAD_10_INV all_outputs()
set_input_delay -clock "clk" -max 0.7 { "icu_req" };
set_input_delay -clock "clk" -max 0.8 { "icu_addr" };
set_input_delay -clock "clk" -max 1.3 { "icu_type" };
set_input_delay -clock "clk" -max 1.3 { "icu_size" };
set_input_delay -clock "clk" -max 0.7 { "dcu_req" };
set_input_delay -clock "clk" -max 0.8 { "dcu_addr" };
set_input_delay -clock "clk" -max 1.3 { "dcu_type" };
set_input_delay -clock "clk" -max 1.3 { "dcu_size" };
set_input_delay -clock "clk" -max 0.7 { "pj_data_in" };
set_input_delay -clock "clk" -max 0.7 { "pj_ack" };
set_input_delay -clock "clk" -max 1.3 { "dcu_dataout" };
 
set_output_delay -clock "clk" -max 4.6 { "biu_icu_ack" };
set_output_delay -clock "clk" -max 4.6 { "biu_data" };
set_output_delay -clock "clk" -max 3.5 { "biu_dcu_ack" };
set_output_delay -clock "clk" -max 3.5 { "pj_addr" };
set_output_delay -clock "clk" -max 4.2 { "pj_data_out" };
set_output_delay -clock "clk" -max 4.6 { "pj_tv" };
set_output_delay -clock "clk" -max 3.5 { "pj_type" };
set_output_delay -clock "clk" -max 3.5 { "pj_ale" };
set_output_delay -clock "clk" -max 3.5 { "pj_size" };
